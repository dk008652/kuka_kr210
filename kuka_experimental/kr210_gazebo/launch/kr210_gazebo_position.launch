<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find kr210_gazebo)/config/kr210_gazebo_joint_position.yaml" command="load"/>


  <!-- load the controllers -->
  <node name="position_controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
	output="screen" ns="/kuka_kr210" args="joint1_position_controller
					  	  joint2_position_controller
					          joint3_position_controller
					          joint4_position_controller
					          joint5_position_controller
					          joint6_position_controller
					          "/>

</launch>
